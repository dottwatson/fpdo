{
    "name": "dottwatson/fpdo",
    "description": "A MySQL engine written in PHP for interct with any source full compatible with Laravel",
    "type": "library",
    "keywords": ["laravel","sql","data","parser","pdo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mirko Temperini",
            "email": "mirko.temperini@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8",
        "ext-pdo": "*",
        "dottwatson/php-mysql-engine": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dottwatson/php-mysql-engine"
        }
    ],    
    "autoload": {
        "psr-4": {
            "Fpdo\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fpdo\\FpdoServiceProvider"
            ]
        }
    }
}